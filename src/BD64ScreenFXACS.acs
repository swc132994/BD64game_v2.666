}

Script "BloodSplashTrail" (VOID)
{
	TakeInventory("BloodTrail",1);
	if	(GetCvar("bd64_BloodTrail") == 1 || GetCvar("bd64_ScreenFX") == 2) { GiveInventory("BloodTrail",1); }
}

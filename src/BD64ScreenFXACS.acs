Script "ScreenEffects" (VOID)
{
	TakeInventory("ScreenFX",1);
	
	//if	(GetCvar("bd64_ScreenFX") == 0) { GiveInventory("ScreenFX",1); }	//Off Mode
	if	(GetCvar("bd64_ScreenFX") == 1) { delay(1); }	//Regular Mode

ACTOR 64HeadCandles : HeadCandles Replaces HeadCandles
{
Scale 0.8
	States
	{
	Spawn:
		HDCA A 0
		TNT1 A 0 A_Jump(128,"Spawn2")
		TNT1 A 0 A_SpawnItemEx("64SmallBloodPool")
		TNT1 A 0 A_SpawnItemEx("LightShaft2BrighterLess")
		TNT1 A 0 A_SpawnItemEx("LightShaft2Fast")
		TNT1 A 0 A_SpawnItemEx("Swarm",0,0,20)
		Goto Stay
	Stay:
		HDCA AB 3 Bright Light("SKULLCANDLES")
		Loop
	Spawn2:		//Burning marine
		ALVE A 1
	SemiStay2:
		TNT1 AAA 0 A_SpawnItemEx("BurningBarrelSmoke")
		TNT1 A 0 A_TakeInventory("BurningBarrelSmokeCheck",0)
	Stay2:
		TNT1 A 0 A_CheckSight("StayBlank2")
		ALVE A 3 Light("BIGREDTORCH") A_CustomMissile("RealFlameTrailsSmall",0,0,random(0,360),2,random(70,110))
		ALVE A 3 Light("BIGREDTORCH") A_CustomMissile("RealFlameTrailsSmall",0,0,random(0,360),2,random(70,110))
		ALVE A 3 Light("BIGREDTORCH") A_CustomMissile("RealFlameTrailsSmall",0,0,random(0,360),2,random(70,110))
		ALVE A 3 Light("BIGREDTORCH") A_CustomMissile("RealFlameTrailsSmall",0,0,random(0,360),2,random(70,110))
		ALVE A 4 Light("BIGREDTORCH") A_CustomMissile("RealFlameTrailsSmall",0,0,random(0,360),2,random(70,110))
		TNT1 A 0 A_GiveInventory("BurningBarrelSmokeCheck",1)
		TNT1 A 0 A_JumpIfInventory("BurningBarrelSmokeCheck",8,"SemiStay2")
		Loop
	StayBlank2:
		ALVE A 3 Bright Light("BIGREDTORCH")
		Goto Stay2
	}
}

